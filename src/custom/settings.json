{
    "workbench.colorTheme": "Default Light+",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.color": "#d8c034",
    "material-icon-theme.folders.theme": "classic",
    "window.zoomLevel": -0.5, 
    "editor.fontSize": 14,
    "files.exclude": {
        "**/.sencha": true,
        "**/.vs": true,
        "**/cvs": true
    },
    "diffEditor.renderSideBySide": true,
    "npm.enableScriptExplorer": true,
    "npm.exclude":[ "**/ext/**", "**/packages/**", "**/.vscode-test/**", "**/build/**" ],
    "svn.defaultCheckoutDirectory": "c:\\Projects",
    "taskExplorer.enableSideBar": true,
    "taskExplorer.pathToAnsicon": "c:\\code\\ansicon\\x64\\ansicon.exe",
    "taskExplorer.pathToAnt": "c:\\code\\ant\\bin\\ant.bat",
    "taskExplorer.includeAnt": [
        "**/*.xml"
    ],
    "taskExplorer.exclude": [
        "**/ext/**",
        "**/packages/**",
        "**/.vscode-test/**",
        "**/test/**",
        "**/build/**",
        "**/sdks/**",
        "**/vendor/**"
    ],
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true
}
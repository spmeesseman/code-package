{
	"name": "codepackage",
	"displayName": "Code Package",
	"description": "Bundled Code Package",
	"publisher": "spmeesseman",
	"version": "1.0.0",
	"icon": "images/subversion.png",
	"homepage": "https://github.com/spmeesseman/codepackage/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/spmeesseman/codepackage.git"
	},
	"bugs": {
		"url": "https://github.com/spmeesseman/codepackage/issues"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^3.0.2",
		"@semantic-release/git": "^7.0.8",
		"commitizen": "^3.0.7",
		"cz-conventional-changelog": "^2.1.0",
		"semantic-release": "^15.13.3",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"scripts": {
		"compile": "ant -logger org.apache.tools.ant.listener.AnsiColorLogger Extract",
		"vscode:prepublish": "npm run compile",
		"semantic-release": "semantic-release --no-ci --allow-same-version",
		"semantic-release-dry": "semantic-release --dry-run",
		"commit": "npx git-cz" 
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
